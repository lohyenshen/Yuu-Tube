<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="160.0" prefWidth="492.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Confirmation_changePassword">
    <children>
        <Label layoutX="113.0" layoutY="40.0" text="Are you sure you want to change password?" textFill="WHITE">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <ImageView fitHeight="55.0" fitWidth="63.0" layoutX="40.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Photo/error.png" />
            </image>
        </ImageView>
        <Button layoutX="148.0" layoutY="100.0" mnemonicParsing="false" onAction="#toChangePassword" prefHeight="26.0" prefWidth="55.0" style="-fx-background-color: white;" text="Yes">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
        <Button layoutX="272.0" layoutY="100.0" mnemonicParsing="false" onAction="#stayProfile" prefHeight="26.0" prefWidth="55.0" style="-fx-background-color: white;" text="Cancel">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
    </children>
</AnchorPane>
