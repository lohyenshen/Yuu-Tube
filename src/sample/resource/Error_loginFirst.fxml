<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="250.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Error_loginFirst">
    <children>
        <Label layoutX="89.0" layoutY="40.0" text="Please login first." textFill="WHITE">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <ImageView fitHeight="55.0" fitWidth="63.0" layoutX="28.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Photo/error.png" />
            </image>
        </ImageView>
        <Button layoutX="147.0" layoutY="96.0" mnemonicParsing="false" onAction="#backToHomePage" prefHeight="26.0" prefWidth="55.0" style="-fx-background-color: white;" text="Okay">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="55.0" layoutY="96.0" mnemonicParsing="false" onAction="#toLogin" prefHeight="26.0" prefWidth="55.0" style="-fx-background-color: white;" text="Log in">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
    </children>
</AnchorPane>
