<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="273.0" prefWidth="474.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.ChangeEmail">
   <children>
      <Label layoutX="107.0" layoutY="62.0" text="You're changing your email" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="107.0" layoutY="134.0" text="New Email:" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="newEmail" layoutX="217.0" layoutY="131.0" />
      <Button layoutX="142.0" layoutY="200.0" mnemonicParsing="false" onAction="#changeEmail" style="-fx-background-color: white;" text="Change">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="257.0" layoutY="200.0" mnemonicParsing="false" onAction="#stayProfile" style="-fx-background-color: white;" text="Cancel">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="changeEmail_error" alignment="CENTER" layoutX="80.0" layoutY="168.0" prefHeight="19.0" prefWidth="328.0" textFill="RED">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
